    "vite": "^4.5.0",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-pwa": "^0.16.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
