    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.27",
    "@iconify-json/carbon": "^1.1.21",
    "@types/node": "^20.8.7",
    "@types/uuid": "^9.0.6",
    "@unocss/eslint-config": "^0.56.5",
    "@unocss/extractor-pug": "^0.56.5",
    "@unocss/reset": "^0.56.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue-macros/volar": "^0.17.0",
    "@vue/language-plugin-pug": "^1.8.19",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.52.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "naive-ui": "^2.35.0",
    "pnpm": "^8.9.2",
    "pug": "^3.0.2",
    "sass": "^1.69.4",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.4",
    "typescript": "^5.2.2",
    "unocss": "^0.56.5",
